#!/usr/local/bin/perl -w
# $HopeName: !script:commands:clib(trunk.8) $
#
# Compile all the .c files in $testlib_dir,
# putting the resulting .o files in $obj_dir.
#

&requiredoptions(
 "MPS_INCLUDE_DIR",
 "MPS_LINK_OBJ",
# "MPS_INTERFACE_VERSION",
 "PLATFORM"
);

&linkobjtimes();

&requiredoptions(
 "MPS_PRODUCT"
);

&clib;

